{
    "common": {
        "name":                     "mihome-vacuum",
        "version": "0.1.0",
        "news": {
			"0.1.0": {
                "en": 				"Inital version",
                "de": 				"Erste Version",
                "ru": 				"Первая версия"
            }
        },
        "title":                    "mihome-vacuum Adapter",
        "desc":                     "This adapter allows control Xiaomi Vacuum cleaner",
        "authors": [
            "bluefox <dogafox@gmail.com>"
        ],
        "license":                  "MIT",
        "platform":                 "Javascript/Node.js",
        "mode":                     "daemon",
        "loglevel":                 "info",
        "icon":                     "mihome-vacuum.png",
        "webExtension":             "lib/mihome-vacuum.js",
        "readme":                   "https://github.com/ioBroker/ioBroker.mihome-vacuum/blob/master/README.md",
        "keywords":                 ["url", "html", "file", "mihome-vacuum"],
        "extIcon":                  "https://raw.githubusercontent.com/ioBroker/ioBroker.mihome-vacuum/master/admin/mihome-vacuum.png",
		"type":                     "common",
        "config":{
            "minWidth": 800,
            "width ": 1224
        }
    },
    "native": {
        "ip":       "",
        "port":     54321,
        "ownPort":  56363,
        "start":    "",
        "pause":    "",
        "home":     "",
        "find":     "",
        "pingInterval": 20000,
        "level1":   "",
        "level2":   "",
        "level3":   ""
    },
    "instanceObjects": [
        {
            "_id":  "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id":  "info.connection",
            "type": "state",
            "common": {
                "role":  "indicator.connected",
                "name":  "If connected to CUL",
                "type":  "boolean",
                "read":  true,
                "write": false,
                "def":   false
            },
            "native": {}
        },
        {
            "_id":  "start",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Start vacuum",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "pause",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Pause vacuum",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "home",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Go home",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "find",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Find",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "state",
            "type": "state",
            "common": {
                "role":  "switch",
                "name":  "Start/pause",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "level",
            "type": "state",
            "common": {
                "role":  "state",
                "name":  "Power level 1-3",
                "type":  "number",
                "min":   1,
                "max":   3,
                "read":  false,
                "write": true
            },
            "native": {}
        }
    ]
    
}
